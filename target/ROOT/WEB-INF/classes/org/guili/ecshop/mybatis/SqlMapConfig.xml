<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd"> 
<configuration>
	<!-- 对象别名 ，用在mapper里面代替整个对象的访问-->
    <typeAliases>
       <typeAlias alias="tmallAnalyzeBean" type="org.guili.ecshop.bean.credit.tmall.TmallAnalyzeBean"/>
       <typeAlias alias="topStore" type="org.guili.ecshop.bean.spider.TopStore"/>
    </typeAliases> 
     
	<mappers>
		<!-- 数据库映射文件 -->
	    <mapper resource="org/guili/ecshop/mybatis/mapper/TmallAnalyzeBeanMapper.xml"/>
	     <mapper resource="org/guili/ecshop/mybatis/mapper/topStoreMapper.xml"/>
	</mappers>
</configuration> 